<!--pages/index/mobans.wxml-->
<view class="tunnel"bindtap="bindViewTap">
<text class="userinfo-nickname">流程设置</text></view>
  <form bindsubmit="formSubmit"name="pushMsgFm" report-submit="true">
    <view class="tunnel"><text class="tunnel-text">流程标题：</text><input name="title" class="input" placeholder="please input here" /></view>
    <view class="tunnel"><text class="tunnel-text">上步代理人：</text><input name="name" class="input" placeholder="please input here" /></view>
    <view class="tunnel"><text class="tunnel-text">单据类型：</text><input name="types" class="input" placeholder="please input here" /></view>
   <view class="tunnel"><text class="tunnel-text"> 内容概述：</text><input name="summary" class="input" placeholder="please input here" /></view>
   <view class="tunnel"><text class="tunnel-text"> 申请人：</text><input name="aname" class="input" placeholder="please input here" /></view>
   <view class="tunnel"><text class="tunnel-text"> 申请时间：</text><input name="time" class="input" placeholder="please input here" /></view>
    <view class="tunnel"><text class="tunnel-text">申请部门：</text><input name="departement" class="input" placeholder="please input here" /></view>
    <view class="tunnel"><text class="tunnel-text">动作处理：</text><input name="action" class="input" placeholder="please input here" /></view>
    <view class="tunnel"><text class="tunnel-text">待办动作：</text><input name="daction" class="input" placeholder="please input here" /></view>
    <view class="tunnel"><text class="tunnel-text">待办时间：</text><input name="dtime" class="input" placeholder="please input here" /></view>
     <view class="tunnel"><button form-type="submit">Submit</button>
    <button form-type="reset">Reset</button></view>
  </form>
