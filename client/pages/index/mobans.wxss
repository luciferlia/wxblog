/* pages/index/mobans.wxss */
page {
    background: #F6F6F6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}
.input{
  border:3px solid #2E8B57;
  border-radius: 10rpx;
  min-height: 60rpx;
  }
  .tunnel {
    margin-top: 40rpx;
    height: 100rpx;
    width: 100%;
    background: #FFF;
    border: 1px solid rgba(0, 0, 0, .1);
    border-left: none;
    border-right: none;
    display: flex;
    flex-direction: row;
    align-items: center;
    transition: all 300ms ease;
}
.tunnel-text {
    width: 200rpx;
    line-height: 42px;
    font-size: 24rpx;
    color: #007AFF;
}