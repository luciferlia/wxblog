page {
    background: #F6F6F6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}
.uploader {
    margin-top: 20rpx;
    height: 100rpx;
    width: 100%;
    background: #FFF;
    border: 1px solid rgba(0, 0, 0, .1);
    border-left: none;
    border-right: none;
    display: flex;
    flex-direction: row;
    align-items: center;
    transition: all 300ms ease;
}
.uploader-text{
    width: 100%;
    line-height: 32px;
    margin-left:25rpx;
    font-size: 27rpx;
    color: #007AFF;
}


